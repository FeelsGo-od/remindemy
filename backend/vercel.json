{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "https://remindemy.vercel.app" }
            ]
          },
          {
            "source": "/https://remindemy-react.vercel.app/(.*)",
            "headers": [
              { "key": "Access-Control-Allow-Origin", "value": "https://remindemy.vercel.app" }
            ]
          } 
    ]
}